def start(self, atts):
    try:
        self.segmentName = atts['full-name']
    except KeyError:
        self.segmentName = atts['name']
    self.layerData = {None: {}, }
    self.addData('start', float(atts['start']))
    self.addData('end', float(atts['end']))
    self.addData('track', int(atts['track']))
    self.addData('name', self.segmentName)
