def parseFile(self, fname):
    fd = zopen(fname, 'r')
    data = fd.read()
    zclose(fd)
    self.reset()
    if data.startswith('<?xml'):
        self.parser.feed(data)
    else:
        self.parser.feed('<?xml version="1.0" encoding="ISO-8859-1"?>')
        self.parser.feed('<sprint>')
        self.parser.feed(data)
        self.parser.feed('</sprint>')
    self.parser.close()
