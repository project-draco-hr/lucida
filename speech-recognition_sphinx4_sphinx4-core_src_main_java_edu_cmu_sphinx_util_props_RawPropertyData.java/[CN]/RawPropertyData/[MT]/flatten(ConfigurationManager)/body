{
  RawPropertyData copyRPD=new RawPropertyData(name,className);
  for (  Map.Entry<String,Object> entry : properties.entrySet()) {
    Object propVal=entry.getValue();
    if (propVal instanceof String) {
      if (((String)propVal).startsWith("${"))       propVal=cm.getGloPropReference(ConfigurationManagerUtils.stripGlobalSymbol((String)propVal));
    }
    copyRPD.properties.put(entry.getKey(),propVal);
  }
  return copyRPD;
}
