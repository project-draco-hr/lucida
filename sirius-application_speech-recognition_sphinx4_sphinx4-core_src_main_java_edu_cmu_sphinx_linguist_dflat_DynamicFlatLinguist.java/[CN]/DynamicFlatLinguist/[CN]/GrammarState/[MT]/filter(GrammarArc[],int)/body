{
  if (nextBase != ANY) {
    List<GrammarArc> list=new ArrayList<GrammarArc>();
    for (    GrammarArc arc : arcs) {
      GrammarNode node=arc.getGrammarNode();
      if (hasEntryContext(node,nextBase)) {
        list.add(arc);
      }
    }
    arcs=list.toArray(new GrammarArc[list.size()]);
  }
  return arcs;
}
