{
  if (transport.write(frameBuffer) < 0) {
    throw new IOException("Write call frame failed");
  }
  if (frameBuffer.remaining() == 0) {
    if (isOneway) {
      cleanUpAndFireCallback(key);
    }
 else {
      state=State.READING_RESPONSE_SIZE;
      sizeBuffer.rewind();
      key.interestOps(SelectionKey.OP_READ);
    }
  }
}
