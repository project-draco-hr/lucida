{
  StringBuilder sb=new StringBuilder("learn_args(");
  boolean first=true;
  sb.append("LUCID:");
  if (this.LUCID == null) {
    sb.append("null");
  }
 else {
    sb.append(this.LUCID);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("knowledge:");
  if (this.knowledge == null) {
    sb.append("null");
  }
 else {
    sb.append(this.knowledge);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
