{
  try {
    TNonblockingServerTransport transport=new TNonblockingServerSocket(port);
    TNonblockingServer.Args args=new TNonblockingServer.Args(transport).processor(processor).protocolFactory(new TBinaryProtocol.Factory()).transportFactory(new TFramedTransport.Factory());
    final TNonblockingServer server=new TNonblockingServer(args);
    System.out.println("Start listening to requests at port " + port + "...");
    server.serve();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
