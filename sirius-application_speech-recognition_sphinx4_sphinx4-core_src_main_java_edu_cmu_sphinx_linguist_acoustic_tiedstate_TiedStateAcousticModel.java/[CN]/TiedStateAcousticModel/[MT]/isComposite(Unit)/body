{
  if (unit.isFiller()) {
    return false;
  }
  Context context=unit.getContext();
  if (context instanceof LeftRightContext) {
    LeftRightContext lrContext=(LeftRightContext)context;
    if (lrContext.getRightContext() == null) {
      return true;
    }
    if (lrContext.getLeftContext() == null) {
      return true;
    }
  }
  return false;
}
