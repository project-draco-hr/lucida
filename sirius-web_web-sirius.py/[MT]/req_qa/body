def req_qa(text, return_dict=None):
    start = time.time()
    words = text.split()
    query = ''
    for w in words:
        query += (w + '%20')
    if (not query):
        return query
    q = ((make_server(QA, ports[0]) + '?query=') + query)
    try:
        resp = urllib2.urlopen(q)
        answer = resp.read()
    except BadStatusLine:
        answer = ''
    print ('QA time: %f' % float((time.time() - start)))
    print text
    print answer
    return answer
