{
  StringBuilder sb=new StringBuilder();
  sb.append("Fst(start=" + start + ", isyms="+ isyms+ ", osyms="+ osyms+ ", semiring="+ semiring+ ")\n");
  int numStates=states.length;
  for (int i=0; i < numStates; i++) {
    State s=states[i];
    sb.append("  " + s + "\n");
    int numArcs=s.getNumArcs();
    for (int j=0; j < numArcs; j++) {
      Arc a=s.getArc(j);
      sb.append("    " + a + "\n");
    }
  }
  return sb.toString();
}
