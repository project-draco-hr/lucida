{
  DoubleData last;
  if (bufferPosition > 0) {
    last=cepstraBuffer[bufferPosition - 1];
  }
 else   if (bufferPosition == 0) {
    last=cepstraBuffer[cepstraBuffer.length - 1];
  }
 else {
    throw new Error("BufferPosition < 0");
  }
  for (int i=0; i < window; i++) {
    addCepstrum(last);
  }
  return window;
}
