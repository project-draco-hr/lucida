{
  if (useTable) {
    int intIndex=(int)(index + 0.5);
    if (0 <= intIndex) {
      if (intIndex < theAddTable.length) {
        return theAddTable[intIndex];
      }
 else {
        return 0.0f;
      }
    }
 else {
      throw new IllegalArgumentException("addTable index has " + "to be negative");
    }
  }
 else {
    return addTableActualComputation(index);
  }
}
