def callback(self):
    if ('code' not in request.args):
        return (None, None, None)
    oauth_session = self.service.get_auth_session(data={'code': request.args['code'], 'grant_type': 'authorization_code', 'redirect_uri': self.get_callback_url(), }, decoder=json.loads)
    me = oauth_session.get('').json()
    return (me['name'], me['email'])
