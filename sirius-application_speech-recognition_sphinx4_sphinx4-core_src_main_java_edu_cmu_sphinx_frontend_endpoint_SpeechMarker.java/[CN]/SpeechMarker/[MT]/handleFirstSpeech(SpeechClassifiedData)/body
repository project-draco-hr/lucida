{
  int speechTime=getAudioTime(audio);
  while (speechTime < startSpeechTime) {
    Data next=readData();
    if (next == null) {
      return false;
    }
    sendToQueue(next);
    if (next instanceof SpeechClassifiedData) {
      if (!((SpeechClassifiedData)next).isSpeech()) {
        return false;
      }
 else {
        speechTime+=getAudioTime(audio);
      }
    }
  }
  return true;
}
