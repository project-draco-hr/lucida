{
  float energy=0;
  Data input=null;
  do {
    input=getPredecessor().getData();
    if (input == null || !(input instanceof DoubleData))     return input;
    energy=0.0f;
    for (    double d : ((DoubleData)input).getValues()) {
      energy+=d * d;
    }
  }
 while (energy < maxEnergy);
  return input;
}
