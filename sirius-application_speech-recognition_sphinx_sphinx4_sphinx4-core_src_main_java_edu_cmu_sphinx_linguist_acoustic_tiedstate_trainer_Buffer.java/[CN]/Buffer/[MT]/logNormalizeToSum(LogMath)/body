{
  assert isLog;
  float logZero=LogMath.LOG_ZERO;
  float den=logZero;
  for (  double val : numerator) {
    if (val != logZero) {
      den=logMath.addAsLinear(den,(float)val);
    }
  }
  for (int i=0; i < numerator.length; i++) {
    if (numerator[i] != logZero) {
      numerator[i]-=den;
    }
  }
  denominator=0.0;
}
