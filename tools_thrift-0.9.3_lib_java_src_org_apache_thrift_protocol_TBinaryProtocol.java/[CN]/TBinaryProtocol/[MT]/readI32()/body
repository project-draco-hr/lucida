{
  byte[] buf=i32rd;
  int off=0;
  if (trans_.getBytesRemainingInBuffer() >= 4) {
    buf=trans_.getBuffer();
    off=trans_.getBufferPosition();
    trans_.consumeBuffer(4);
  }
 else {
    readAll(i32rd,0,4);
  }
  return ((buf[off] & 0xff) << 24) | ((buf[off + 1] & 0xff) << 16) | ((buf[off + 2] & 0xff) << 8)| ((buf[off + 3] & 0xff));
}
