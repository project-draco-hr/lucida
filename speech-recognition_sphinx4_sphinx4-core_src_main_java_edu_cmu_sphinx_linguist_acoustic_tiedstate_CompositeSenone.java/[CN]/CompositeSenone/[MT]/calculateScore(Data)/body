{
  float logScore;
  if (wantMaxScore) {
    logScore=-Float.MAX_VALUE;
    for (    Senone senone : senones) {
      logScore=Math.max(logScore,senone.getScore(feature));
    }
  }
 else {
    logScore=0.0f;
    for (    Senone senone : senones) {
      logScore+=senone.getScore(feature);
    }
    logScore=logScore / senones.length;
  }
  return logScore + weight;
}
