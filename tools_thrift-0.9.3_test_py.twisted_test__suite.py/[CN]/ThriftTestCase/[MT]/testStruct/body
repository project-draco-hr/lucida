@defer.inlineCallbacks
def testStruct(self):
    x = Xtruct()
    x.string_thing = 'Zero'
    x.byte_thing = 1
    x.i32_thing = (-3)
    x.i64_thing = (-5)
    y = yield self.client.testStruct(x)
    self.assertEquals(y.string_thing, 'Zero')
    self.assertEquals(y.byte_thing, 1)
    self.assertEquals(y.i32_thing, (-3))
    self.assertEquals(y.i64_thing, (-5))
