{
  Runnable invocation=getRunnable(frameBuffer);
  if (invoker != null) {
    try {
      invoker.execute(invocation);
      return true;
    }
 catch (    RejectedExecutionException rx) {
      LOGGER.warn("ExecutorService rejected execution!",rx);
      return false;
    }
  }
 else {
    invocation.run();
    return true;
  }
}
