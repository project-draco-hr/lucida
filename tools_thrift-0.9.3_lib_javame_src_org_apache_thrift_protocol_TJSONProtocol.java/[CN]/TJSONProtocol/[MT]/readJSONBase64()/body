{
  TByteArrayOutputStream arr=readJSONString(false);
  byte[] b=arr.get();
  int len=arr.len();
  int off=0;
  int size=0;
  while (len >= 4) {
    TBase64Utils.decode(b,off,4,b,size);
    off+=4;
    len-=4;
    size+=3;
  }
  if (len > 1) {
    TBase64Utils.decode(b,off,len,b,size);
    size+=len - 1;
  }
  byte[] result=new byte[size];
  System.arraycopy(b,0,result,0,size);
  return result;
}
