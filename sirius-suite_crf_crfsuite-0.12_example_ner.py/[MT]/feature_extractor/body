def feature_extractor(X):
    for x in X:
        observation(x)
    crfutils.apply_templates(X, templates)
    for t in range(len(X)):
        disjunctive(X, t, 'w', (-4), (-1))
        disjunctive(X, t, 'w', 1, 4)
    if X:
        X[0]['F'].append('__BOS__')
        X[(-1)]['F'].append('__EOS__')
