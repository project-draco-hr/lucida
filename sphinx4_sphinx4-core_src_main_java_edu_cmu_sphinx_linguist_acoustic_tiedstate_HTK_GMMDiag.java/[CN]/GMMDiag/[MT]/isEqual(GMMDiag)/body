{
  if (getNgauss() != g.getNgauss())   return false;
  if (getNgauss() != g.getNcoefs())   return false;
  for (int i=0; i < getNgauss(); i++) {
    if (isDiff(getWeight(i),g.getWeight(i)))     return false;
    for (int j=0; j < getNcoefs(); j++) {
      if (isDiff(getMean(i,j),g.getMean(i,j)))       return false;
      if (isDiff(getVar(i,j),g.getVar(i,j)))       return false;
    }
  }
  return true;
}
