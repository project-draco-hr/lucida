{
  Token token=this;
  List<Token> list=new ArrayList<Token>();
  while (token != null) {
    list.add(token);
    token=token.getPredecessor();
  }
  for (int i=list.size() - 1; i >= 0; i--) {
    token=list.get(i);
    if (includeHMMStates || (!(token.getSearchState() instanceof HMMSearchState))) {
      System.out.println("  " + token);
    }
  }
  System.out.println();
}
