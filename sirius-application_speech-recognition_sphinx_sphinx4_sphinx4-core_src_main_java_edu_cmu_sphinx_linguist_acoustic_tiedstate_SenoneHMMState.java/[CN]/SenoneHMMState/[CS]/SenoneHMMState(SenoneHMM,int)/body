{
  this.hmm=hmm;
  this.state=which;
  this.isEmitting=((hmm.getTransitionMatrix().length - 1) != state);
  if (isEmitting) {
    SenoneSequence ss=hmm.getSenoneSequence();
    senone=ss.getSenones()[state];
  }
  Utilities.objectTracker("HMMState",objectCount++);
  hashCode=hmm.hashCode() + 37 * state;
}
