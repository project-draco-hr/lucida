{
  boolean done=false;
  Result result=null;
  streamEnd=false;
  for (int i=0; i < nFrames && !done; i++) {
    done=recognize();
  }
  if (activeList.getBestToken() != null) {
    ActiveList fixedList=undoLastGrowStep();
    if (!streamEnd)     result=new Result(fixedList,resultList,currentFrameNumber,done);
  }
  if (showTokenCount) {
    showTokenCount();
  }
  return result;
}
