def get_address(filename, address):
    '\n    Retrieve an AddressInfo object for the specified object file and address.\n\n    Keeps a global list of AddressInfo objects.  Two calls to get_address()\n    with the same filename and address will always return the same AddressInfo\n    object.\n    '
    global g_addrs_by_filename
    try:
        by_address = g_addrs_by_filename[filename]
    except KeyError:
        by_address = {}
        g_addrs_by_filename[filename] = by_address
    try:
        addr_info = by_address[address]
    except KeyError:
        addr_info = AddressInfo(filename, address)
        by_address[address] = addr_info
    return addr_info
