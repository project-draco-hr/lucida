{
  try {
    File file=new File(fileName);
    float start=getSeconds(totalBytes);
    totalBytes+=file.length();
    float end=getSeconds(totalBytes);
    transcript.write(context + " 1 " + fileName+ ' '+ start+ ' '+ end+ "  "+ reference+ '\n');
    transcript.flush();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}
