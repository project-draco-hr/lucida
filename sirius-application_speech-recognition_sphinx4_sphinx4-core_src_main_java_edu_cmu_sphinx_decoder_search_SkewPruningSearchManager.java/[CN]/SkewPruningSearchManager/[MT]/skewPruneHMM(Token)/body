{
  boolean keep=true;
  SearchState ss=t.getSearchState();
  if (ss instanceof HMMSearchState) {
    if (!t.isEmitting()) {
      Token lastToken=skewMap.get(ss);
      if (lastToken != null) {
        int lastFrame=lastToken.getFrameNumber();
        if (t.getFrameNumber() - lastFrame > skew || t.getScore() > lastToken.getScore()) {
          keep=true;
        }
 else {
          logger.fine("Dropped " + t + " in favor of "+ lastToken);
          keep=false;
        }
      }
 else {
        keep=true;
      }
      if (keep) {
        skewMap.put(ss,t);
      }
    }
  }
  return keep;
}
