{
  context_.write();
  String str=Long.toString(num);
  boolean escapeNum=context_.escapeNum();
  if (escapeNum) {
    trans_.write(QUOTE);
  }
  try {
    byte[] buf=str.getBytes("UTF-8");
    trans_.write(buf);
  }
 catch (  UnsupportedEncodingException uex) {
    throw new TException("JVM DOES NOT SUPPORT UTF-8");
  }
  if (escapeNum) {
    trans_.write(QUOTE);
  }
}
