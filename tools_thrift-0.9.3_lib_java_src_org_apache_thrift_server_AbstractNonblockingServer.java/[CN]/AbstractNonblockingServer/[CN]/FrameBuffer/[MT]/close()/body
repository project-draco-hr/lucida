{
  if (state_ == FrameBufferState.READING_FRAME || state_ == FrameBufferState.READ_FRAME_COMPLETE || state_ == FrameBufferState.AWAITING_CLOSE) {
    readBufferBytesAllocated.addAndGet(-buffer_.array().length);
  }
  trans_.close();
  if (eventHandler_ != null) {
    eventHandler_.deleteContext(context_,inProt_,outProt_);
  }
}
