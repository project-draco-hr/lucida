{
  if (rules.size() == 0) {
    return "<NULL>";
  }
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < rules.size(); ++i) {
    if (i > 0)     sb.append(' ');
    JSGFRule r=rules.get(i);
    if ((r instanceof JSGFRuleAlternatives) || (r instanceof JSGFRuleSequence))     sb.append("( ").append(r).append(" )");
 else {
      sb.append(r);
    }
  }
  return sb.toString();
}
