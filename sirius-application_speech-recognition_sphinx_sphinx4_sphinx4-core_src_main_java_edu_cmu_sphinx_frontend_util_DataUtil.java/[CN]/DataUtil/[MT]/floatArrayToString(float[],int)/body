{
  StringBuilder dump=new StringBuilder().append(data.length);
  for (  float val : data) {
    if (format == DECIMAL) {
      dump.append(' ').append(formatDouble(val,decimalIntegerDigits,decimalFractionDigits));
    }
 else     if (format == HEXADECIMAL) {
      int binary=Float.floatToRawIntBits(val);
      dump.append(" 0x").append(Integer.toHexString(binary));
    }
 else     if (format == SCIENTIFIC) {
      dump.append(' ').append(Utilities.doubleToScientificString(val,floatScientificFractionDigits));
    }
  }
  return dump.toString();
}
