{
  if (cachedName == null) {
    StringBuilder sb=new StringBuilder();
    if (isEmitting()) {
      sb.append('*');
    }
    sb.append(getName());
    if (parent != null) {
      sb.append('_');
      sb.append(parent);
    }
    if (isFinal()) {
      sb.append('!');
    }
    cachedName=sb.toString();
  }
  return cachedName;
}
