{
  try {
    System.err.println("Dumping " + title + " to "+ fileName);
    FileWriter f=new FileWriter(fileName);
    f.write("graph: {\n");
    f.write("title: \"" + title + "\"\n");
    f.write("display_edge_labels: yes\n");
    for (    Node node : nodes.values()) {
      node.dumpAISee(f);
    }
    for (    Edge edge : edges) {
      edge.dumpAISee(f);
    }
    f.write("}\n");
    f.close();
  }
 catch (  IOException e) {
    throw new Error(e.toString());
  }
}
