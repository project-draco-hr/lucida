{
  if (canBeUsedNaked()) {
    internalTestNakedI64(0);
  }
  internalTestI64Field(0);
  for (int i=0; i < 62; i++) {
    if (canBeUsedNaked()) {
      internalTestNakedI64(1L << i);
      internalTestNakedI64(-(1L << i));
    }
    internalTestI64Field(1L << i);
    internalTestI64Field(-(1L << i));
  }
}
