{
  if (feature instanceof DoubleData)   System.err.println("DoubleData conversion required on mixture level!");
  float[] featureVector=FloatData.toFloatData(feature).getValues();
  float logTotal=LogMath.LOG_ZERO;
  for (int i=0; i < mixtureComponents.length; i++) {
    logTotal=logMath.addAsLinear(logTotal,mixtureComponents[i].getScore(featureVector) + logMixtureWeights[i]);
  }
  return logTotal;
}
