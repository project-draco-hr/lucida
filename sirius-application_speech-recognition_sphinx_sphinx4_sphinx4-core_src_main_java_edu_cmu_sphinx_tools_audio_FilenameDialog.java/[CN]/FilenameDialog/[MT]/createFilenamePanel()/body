{
  Container contentPane=getContentPane();
  GridBagLayout gridBag=new GridBagLayout();
  GridBagConstraints constraints;
  Insets insets;
  contentPane.setLayout(gridBag);
  filename=new JTextField(12);
  JLabel filenameLabel=new JLabel("Filename:");
  filenameLabel.setLabelFor(filename);
  insets=new Insets(12,12,0,0);
  constraints=new GridBagConstraints(0,0,1,1,0.0,0.0,GridBagConstraints.WEST,GridBagConstraints.NONE,insets,0,0);
  gridBag.setConstraints(filenameLabel,constraints);
  contentPane.add(filenameLabel);
  insets=new Insets(12,7,0,12);
  constraints=new GridBagConstraints(1,0,1,1,1.0,1.0,GridBagConstraints.WEST,GridBagConstraints.HORIZONTAL,insets,0,0);
  gridBag.setConstraints(filename,constraints);
  contentPane.add(filename);
  JButton okButton=new JButton("Save");
  okButton.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      setVisible(false);
    }
  }
);
  insets=new Insets(0,12,12,12);
  constraints=new GridBagConstraints(0,2,2,1,1.0,1.0,GridBagConstraints.EAST,GridBagConstraints.NONE,insets,0,0);
  gridBag.setConstraints(okButton,constraints);
  contentPane.add(okButton);
  getRootPane().setDefaultButton(okButton);
}
