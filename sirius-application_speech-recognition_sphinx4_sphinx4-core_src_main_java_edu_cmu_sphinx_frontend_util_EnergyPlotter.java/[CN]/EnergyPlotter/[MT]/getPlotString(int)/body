{
  char[] plot=new char[energy];
  Arrays.fill(plot,'.');
  if (energy > 0) {
    if (energy < 10) {
      plot[plot.length - 1]=(char)('0' + energy);
    }
 else {
      plot[plot.length - 2]='1';
      plot[plot.length - 1]=(char)('0' + (energy - 10));
    }
  }
  return ('+' + new String(plot));
}
