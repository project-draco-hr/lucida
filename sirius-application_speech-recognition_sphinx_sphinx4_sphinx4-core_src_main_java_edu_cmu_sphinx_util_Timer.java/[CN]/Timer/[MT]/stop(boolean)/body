{
  if (startTime == 0L) {
    notReliable=true;
    System.out.println(getName() + " timer.stop() called without a start()");
  }
  curTime=System.currentTimeMillis() - startTime;
  startTime=0L;
  if (curTime > maxTime) {
    maxTime=curTime;
  }
  if (curTime < minTime) {
    minTime=curTime;
  }
  count++;
  sum+=curTime;
  if (verbose) {
    dump();
  }
  return curTime;
}
