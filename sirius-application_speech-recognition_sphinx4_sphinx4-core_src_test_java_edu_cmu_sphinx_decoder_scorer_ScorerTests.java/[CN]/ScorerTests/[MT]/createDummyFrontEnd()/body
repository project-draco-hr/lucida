{
  DataBufferProcessor bufferProc=ConfigurationManager.getInstance(DataBufferProcessor.class);
  bufferProc.processDataFrame(new DataStartSignal(16000,true));
  for (  DoubleData doubleData : RandomDataProcessor.createFeatVectors(5,16000,0,39,10))   bufferProc.processDataFrame(doubleData);
  bufferProc.processDataFrame(new SpeechStartSignal());
  for (  DoubleData doubleData : RandomDataProcessor.createFeatVectors(3,16000,1000,39,10))   bufferProc.processDataFrame(doubleData);
  bufferProc.processDataFrame(new SpeechEndSignal());
  for (  DoubleData doubleData : RandomDataProcessor.createFeatVectors(5,16000,2000,39,10))   bufferProc.processDataFrame(doubleData);
  bufferProc.processDataFrame(new DataEndSignal(123));
  return bufferProc;
}
