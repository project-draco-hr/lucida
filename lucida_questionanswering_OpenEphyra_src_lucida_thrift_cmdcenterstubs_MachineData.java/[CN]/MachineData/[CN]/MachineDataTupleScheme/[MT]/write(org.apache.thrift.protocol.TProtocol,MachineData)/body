{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetName()) {
    optionals.set(0);
  }
  if (struct.isSetPort()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetName()) {
    oprot.writeString(struct.name);
  }
  if (struct.isSetPort()) {
    oprot.writeI32(struct.port);
  }
}
