{
  if (inputStream_ != null) {
    try {
      inputStream_.close();
    }
 catch (    IOException iox) {
      LOGGER.warn("Error closing input stream.",iox);
    }
    inputStream_=null;
  }
  if (outputStream_ != null) {
    try {
      outputStream_.close();
    }
 catch (    IOException iox) {
      LOGGER.warn("Error closing output stream.",iox);
    }
    outputStream_=null;
  }
}
