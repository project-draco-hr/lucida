def parse_items(self, name):
    assert (name[(-1)] == 's')
    items = {}
    self.element_start(name)
    while (self.token.type == XML_ELEMENT_START):
        (id, values) = self.parse_item(name[:(-1)])
        assert (id not in items)
        items[id] = values
    self.element_end(name)
    return items
