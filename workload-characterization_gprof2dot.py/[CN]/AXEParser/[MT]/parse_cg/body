def parse_cg(self):
    'Parse the call graph.'
    line = self.readline()
    while self._cg_header_re.match(line):
        line = self.readline()
    entry_lines = []
    while (not self._cg_footer_re.match(line)):
        if line.isspace():
            self.parse_cg_entry(entry_lines)
            entry_lines = []
        else:
            entry_lines.append(line)
        line = self.readline()
