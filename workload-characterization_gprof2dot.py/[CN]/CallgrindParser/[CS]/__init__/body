def __init__(self, infile):
    LineParser.__init__(self, infile)
    self.position_ids = {}
    self.positions = {}
    self.num_positions = 1
    self.cost_positions = ['line']
    self.last_positions = [0]
    self.num_events = 0
    self.cost_events = []
    self.profile = Profile()
    self.profile[SAMPLES] = 0
