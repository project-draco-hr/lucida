def parse_association_spec(self):
    line = self.lookahead()
    if (not line.startswith('calls=')):
        return False
    (_, values) = line.split('=', 1)
    values = values.strip().split()
    calls = int(values[0])
    call_position = values[1:]
    self.consume()
    self.parse_cost_line(calls)
    return True
