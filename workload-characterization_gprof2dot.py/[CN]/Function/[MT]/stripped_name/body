def stripped_name(self):
    'Remove extraneous information from C++ demangled function names.'
    name = self.name
    while True:
        (name, n) = self._parenthesis_re.subn('', name)
        if (not n):
            break
    name = self._const_re.sub('', name)
    while True:
        (name, n) = self._angles_re.subn('', name)
        if (not n):
            break
    return name
