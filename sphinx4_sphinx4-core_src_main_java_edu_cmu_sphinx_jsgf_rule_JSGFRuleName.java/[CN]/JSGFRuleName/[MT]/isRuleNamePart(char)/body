{
  if (Character.isJavaIdentifierPart(c)) {
    return true;
  }
  return ((c == '!') || (c == '#') || (c == '%')|| (c == '&')|| (c == '(')|| (c == ')')|| (c == '+')|| (c == ',')|| (c == '-')|| (c == '/')|| (c == ':')|| (c == ';')|| (c == '=')|| (c == '@')|| (c == '[')|| (c == '\\')|| (c == ']')|| (c == '^')|| (c == '|')|| (c == '~'));
}
