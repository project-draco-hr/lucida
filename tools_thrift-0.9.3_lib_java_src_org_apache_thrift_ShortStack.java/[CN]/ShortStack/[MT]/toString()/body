{
  StringBuilder sb=new StringBuilder();
  sb.append("<ShortStack vector:[");
  for (int i=0; i < vector.length; i++) {
    if (i != 0) {
      sb.append(" ");
    }
    if (i == top) {
      sb.append(">>");
    }
    sb.append(vector[i]);
    if (i == top) {
      sb.append("<<");
    }
  }
  sb.append("]>");
  return sb.toString();
}
