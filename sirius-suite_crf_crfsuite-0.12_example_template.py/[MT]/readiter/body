def readiter(fi, sep=None):
    X = []
    for line in fi:
        line = line.strip('\n')
        if (not line):
            yield X
            X = []
        else:
            fields = line.split(sep)
            item = {'x': fields[0:(-1)], 'y': fields[(-1)], 'F': [], }
            X.append(item)
