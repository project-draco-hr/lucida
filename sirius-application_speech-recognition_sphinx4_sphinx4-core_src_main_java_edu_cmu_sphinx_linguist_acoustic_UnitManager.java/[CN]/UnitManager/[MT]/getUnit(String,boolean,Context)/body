{
  Unit unit=ciMap.get(name);
  if (context == Context.EMPTY_CONTEXT) {
    if (unit == null) {
      unit=new Unit(name,filler,nextID++);
      ciMap.put(name,unit);
      if (logger != null && logger.isLoggable(Level.INFO)) {
        logger.info("CI Unit: " + unit);
      }
    }
  }
 else {
    unit=new Unit(unit,filler,context);
  }
  return unit;
}
