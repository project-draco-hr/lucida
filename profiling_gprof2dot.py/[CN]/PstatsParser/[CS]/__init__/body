def __init__(self, *filename):
    import pstats
    try:
        self.stats = pstats.Stats(*filename)
    except ValueError:
        if PYTHON_3:
            sys.stderr.write(('error: failed to load %s\n' % ', '.join(filename)))
            sys.exit(1)
        import hotshot.stats
        self.stats = hotshot.stats.load(filename[0])
    self.profile = Profile()
    self.function_ids = {}
