def _hue_to_rgb(self, m1, m2, h):
    if (h < 0.0):
        h += 1.0
    elif (h > 1.0):
        h -= 1.0
    if ((h * 6) < 1.0):
        return (m1 + (((m2 - m1) * h) * 6.0))
    elif ((h * 2) < 1.0):
        return m2
    elif ((h * 3) < 2.0):
        return (m1 + (((m2 - m1) * ((2.0 / 3.0) - h)) * 6.0))
    else:
        return m1
