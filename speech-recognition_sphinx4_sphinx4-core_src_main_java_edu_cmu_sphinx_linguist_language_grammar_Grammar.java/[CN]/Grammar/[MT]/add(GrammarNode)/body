{
  if (node.getID() > maxIdentity) {
    maxIdentity=node.getID();
  }
  if (idCheck) {
    for (    GrammarNode grammarNode : grammarNodes) {
      if (grammarNode.getID() == node.getID()) {
        throw new Error("DUP ID " + grammarNode + " and "+ node);
      }
    }
  }
  grammarNodes.add(node);
}
