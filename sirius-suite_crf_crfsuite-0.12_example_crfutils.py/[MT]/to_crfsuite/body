def to_crfsuite(X):
    '\n    Convert an item sequence into an object compatible with crfsuite\n    Python module.\n\n    @type   X:      list of mapping objects\n    @param  X:      The sequence.\n    @rtype          crfsuite.ItemSequence\n    @return        The same sequence in crfsuite.ItemSequence type.\n    '
    import crfsuite
    xseq = crfsuite.ItemSequence()
    for x in X:
        item = crfsuite.Item()
        for f in x['F']:
            if isinstance(f, str):
                item.append(crfsuite.Attribute(escape(f)))
            else:
                item.append(crfsuite.Attribute(escape(f[0]), f[1]))
        xseq.append(item)
    return xseq
