{
  if (!inSilence) {
    return readNextDataFile();
  }
 else {
    String next=null;
    if (silenceCount > 0) {
      next=silenceFileName;
      if (transcript != null) {
        writeSilenceToTranscript();
      }
      silenceCount--;
      if (silenceCount <= 0) {
        inSilence=false;
      }
    }
    return next;
  }
}
