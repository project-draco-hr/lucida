{
  try {
    Method method=enumClass.getMethod("findByValue",int.class);
    return (TEnum)method.invoke(null,value);
  }
 catch (  NoSuchMethodException nsme) {
    return null;
  }
catch (  IllegalAccessException iae) {
    return null;
  }
catch (  InvocationTargetException ite) {
    return null;
  }
}
