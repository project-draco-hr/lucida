{
  System.out.print("testException(" + arg + ")\n");
  if (arg.equals("Xception")) {
    Xception x=new Xception();
    x.errorCode=1001;
    x.message=arg;
    throw x;
  }
 else   if (arg.equals("TException")) {
    throw new TException(arg);
  }
 else {
    Xtruct result=new Xtruct();
    result.string_thing=arg;
  }
  return;
}
