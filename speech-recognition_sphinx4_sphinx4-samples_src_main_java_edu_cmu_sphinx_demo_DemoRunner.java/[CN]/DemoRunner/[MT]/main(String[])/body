{
  if (0 == args.length || !classes.containsKey(args[0])) {
    printUsage();
    System.exit(1);
  }
  try {
    Method main=classes.get(args[0]).getMethod("main",paramTypes);
    main.invoke(null,new Object[]{copyOfRange(args,1,args.length)});
  }
 catch (  InvocationTargetException e) {
    throw e.getCause();
  }
}
