def flushTo(self, other_queue):
    if (len(self._queue) == 0):
        pass
    elif (len(self._queue) == 1):
        other_queue.add((('/*' + self._queue[0][2:].rstrip()) + ' */\n'))
    else:
        other_queue.add('/*\n')
        LineQueue.flushTo(self, other_queue)
        other_queue.add('*/\n')
    self.clear()
