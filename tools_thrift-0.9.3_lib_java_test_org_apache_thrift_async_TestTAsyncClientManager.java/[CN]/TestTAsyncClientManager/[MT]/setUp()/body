{
  server_=new THsHaServer(new Args(new TNonblockingServerSocket(new TNonblockingServerSocket.NonblockingAbstractServerSocketArgs().port(ServerTestBase.PORT))).processor(new Srv.Processor(new SrvHandler())));
  serverThread_=new Thread(new Runnable(){
    public void run(){
      server_.serve();
    }
  }
);
  serverThread_.start();
  clientManager_=new TAsyncClientManager();
  Thread.sleep(500);
}
