def read(self, sz):
    'Read up to sz bytes from the decompressed bytes buffer, and\n    read from the underlying transport if the decompression\n    buffer is empty.\n    '
    ret = self.__rbuf.read(sz)
    if (len(ret) > 0):
        return ret
    while True:
        if self.readComp(sz):
            break
    ret = self.__rbuf.read(sz)
    return ret
