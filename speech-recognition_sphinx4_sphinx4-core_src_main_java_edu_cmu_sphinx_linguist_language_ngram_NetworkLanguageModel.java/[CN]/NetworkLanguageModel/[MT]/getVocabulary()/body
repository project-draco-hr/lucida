{
  Set<String> result=new HashSet<String>();
  try {
    BufferedReader reader=new BufferedReader(new InputStreamReader(location.openStream()));
    String line;
    while (true) {
      line=reader.readLine();
      if (line == null)       break;
      if (line.length() == 0)       continue;
      result.add(line.trim());
    }
    reader.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return result;
}
