{
  int length=lst.get(1).length;
  Array2DRowRealMatrix ret=new Array2DRowRealMatrix(size,length);
  int i=0;
  for (i=0; i < size; i++) {
    double[] converted=new double[length];
    for (int j=0; j < length; j++)     converted[j]=((lst.get(i))[j]);
    ret.setRow(i,converted);
  }
  return ret;
}
