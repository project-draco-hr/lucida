{
  a-=b;
  a-=c;
  a^=zeroFill(c,13);
  b-=c;
  b-=a;
  b^=a << 8;
  c-=a;
  c-=b;
  c^=zeroFill(b,13);
  a-=b;
  a-=c;
  a^=zeroFill(c,12);
  b-=c;
  b-=a;
  b^=a << 16;
  c-=a;
  c-=b;
  c^=zeroFill(b,5);
  a-=b;
  a-=c;
  a^=zeroFill(c,3);
  b-=c;
  b-=a;
  b^=a << 10;
  c-=a;
  c-=b;
  c^=zeroFill(b,15);
  return (new int[]{a,b,c});
}
