import pickledb
import subprocess, re, os, sys
if (__name__ == '__main__'):
    pwd = os.getcwd()
    db_name = sys.argv[1]
    ddb = ('matching/%s/db' % db_name)
    pickdb = pickledb.load((db_name + '.pickle'), False)
    imgs = os.listdir(ddb)
    for i in imgs:
        data = os.path.splitext(i)[0].split('-')
        text = ' '.join(data)
        print text
        pickdb.set(i, data)
    pickdb.dump()
