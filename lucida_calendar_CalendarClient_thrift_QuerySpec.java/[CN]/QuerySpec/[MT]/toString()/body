{
  StringBuilder sb=new StringBuilder("QuerySpec(");
  boolean first=true;
  sb.append("name:");
  if (this.name == null) {
    sb.append("null");
  }
 else {
    sb.append(this.name);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("content:");
  if (this.content == null) {
    sb.append("null");
  }
 else {
    sb.append(this.content);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
